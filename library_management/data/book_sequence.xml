<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="seq_book_id" model="ir.sequence">
            <field name="name">Book Sequence</field>
            <field name="code">Book.Sequence</field>
            <field name="prefix">BOOK</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_checkout_id" model="ir.sequence">
            <field name="name">Checkout Sequence</field>
            <field name="code">Checkout.Sequence</field>
            <field name="prefix">CH</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>